# training
nohup python scripts/train_giga.py --dataset data/pile/data_pile_train_constructed_4M --dataset_raw data/pile/data_pile_train_random_raw_4M > nohup_logs/train4M_20ep.out &

# reconstruction only
nohup python scripts/eval_geometry_voxel.py --model-path data/runs/22-11-30-13-52_dataset=data_pile_train_constructed_4M,augment=False,net=giga,batch_size=32,lr=2e-04/best_vgn_giga_val_acc=0.8915.pt --type giga --dataset data/pile/data_pile_train_constructed_4M --dataset_raw data/pile/data_pile_train_random_raw_4M

# grasps visual
nohup python scripts/sim_grasp_multiple.py --num-view 1 --object_set pile/test --scene pile --num-rounds 100 --sideview --add-noise dex --force --best --model data/runs/22-11-30-13-52_dataset=data_pile_train_constructed_4M,augment=False,net=giga,batch_size=32,lr=2e-04/best_vgn_giga_val_acc=0.8915.pt --type giga --result-path data/grasps --vis > nohup_logs/grasps4M.out &

# dataset creation
python scripts/generate_data_parallel.py --scene pile --object-set pile/train --num-grasps 4000000 --num-proc 40 --save-scene ./data/pile/data_pile_train_random_raw_4M

python generate_data_gpg_parallel.py --root /work/scratch/sj93qicy/potato-net/data/pile/data_pile_train_random_raw_4M_GPG_60_PARTIAL --use_previous_scenes True --previous_root /work/scratch/sj93qicy/potato-net/data/pile/data_pile_train_random_raw_4M_GPG_60 --num_proc 96 --grasps_per_scene 60 --grasps_per_scene_gpg 60 --partial_pc True --save_scene True --random True

python generate_data_grasp_surface_clouds.py --raw_root /work/scratch/sj93qicy/potato-net/data/pile/data_pile_train_random_raw_2M_GPG_MIXED --num_proc 96 --save_occ_values True --add_noise True

nohup python scripts/clean_balance_data.py ./data/pile/data_pile_train_random_raw_4M > nohup_logs/dataset4M_2.out &

nohup python scripts/construct_dataset_parallel.py --num-proc 40 --single-view --add-noise dex ./data/pile/data_pile_train_random_raw_4M ./data/pile/data_pile_train_constructed_4M > nohup_logs/dataset4M_3.out &

nohup python scripts/save_occ_data_parallel.py ./data/pile/data_pile_train_random_raw_4M 100000 2 --num-proc 40 > nohup_logs/dataset4M_4.out &

python scripts/sim_grasp_multiple.py --num-view 1 --object_set pile/test --scene pile --num-rounds 100 --add-noise dex --force --best --model data/runs/23-01-19-13-12_dataset=data_pile_train_constructed_4M,augment=False,net=6d_giga,batch_size=32,lr=2e-04/best_vgn_giga_val_acc=0.9986.pt --type giga --result-path data/grasps --vis

## Different views and folders for scenes
python scripts/sim_grasp_multiple.py --num-view 1 --object_set pile/test --scene pile --num-rounds 20 --add-noise dex --force --best --model data/runs_relevant/23-02-14-12-45_dataset\=data_pile_train_constructed_4M_HighRes_radomized_views\,augment\=False\,net\=6d_giga_hr\,batch_size\=48\,lr\=2e-04/best_vgn_giga_hr_val_acc\=0.8891.pt --resolution=64 --type giga_hr --result-path data/grasps --vis --sim-gui

